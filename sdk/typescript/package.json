{
  "name": "@olumi/assistants-sdk",
  "version": "1.5.0",
  "description": "Official TypeScript SDK for Olumi Assistants Service",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && mkdir -p dist/esm dist/cjs && cp dist/index.js dist/cjs/index.js && cp dist/index.js dist/esm/index.js",
    "test": "vitest run",
    "prepublishOnly": "pnpm build",
    "size": "node -e \"try{require('fs').statSync('dist')}catch(e){process.exit(0)};require('child_process').execSync('du -sh dist || true',{stdio:'inherit'})\""
  },
  "keywords": [
    "olumi",
    "assistants",
    "decision-making",
    "llm",
    "typescript",
    "sdk"
  ],
  "author": "Olumi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Talchain/olumi-assistants-service.git",
    "directory": "sdk/typescript"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.6.3",
    "vitest": "^1.6.1"
  }
}
